alias sad=ssh-add\ ~/.ssh/batcheloranator

alias cmdRepo='open -a safari https://github.com/cmdline-batcheloranator' 

# vars
cmdline=~/git/cmdline
chem=~/chemistry_stuff
vscode='Visual Studio Code'
