# exports
export CLICOLOR=1
export LSCOLORS=hxfxcxdxbxegebacacacad
export PATH="/Applications/calibre.app/Contents/MacOS/:$PATH"

# aliases
alias ll='ls -l'
alias la='ls -ahl'

alias gp='git push'
alias gg='git pull'
alias ga='git add .'
alias gs='git status'
alias gc='git commit -m 'x''

alias sd='shutdown -h  +0'
